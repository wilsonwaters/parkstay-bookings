{
  "name": "parkstay-bookings",
  "version": "1.0.0",
  "description": "Automated booking tool for WA ParkStay",
  "main": "dist/main/main/index.js",
  "author": "ParkStay Bookings Team",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "concurrently \"tsc -p tsconfig.main.json --watch\" \"tsc-alias -p tsconfig.main.json --watch\"",
    "dev:renderer": "vite",
    "start": "cross-env ELECTRON_RENDERER_URL=http://localhost:3000 electron .",
    "start:electron": "electron .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json && tsc-alias -p tsconfig.main.json && npm run copy:assets",
    "build:renderer": "vite build",
    "copy:assets": "node -e \"require('fs').cpSync('src/main/database/schema.sql', 'dist/main/main/database/schema.sql')\"",
    "build:all": "npm run build && electron-builder -mwl",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build:win --publish never",
    "dist:mac": "npm run build:mac --publish never",
    "dist:linux": "npm run build:linux --publish never",
    "pack": "npm run build && electron-builder --dir",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md,json}\"",
    "type-check": "tsc --noEmit && tsc -p tsconfig.renderer.json --noEmit",
    "clean": "rimraf dist release out coverage",
    "clean:all": "npm run clean && rimraf node_modules",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install",
    "release": "npm run build && electron-builder --publish always",
    "release:win": "npm run build:win --publish always",
    "release:mac": "npm run build:mac --publish always",
    "release:linux": "npm run build:linux --publish always",
    "version:patch": "npm version patch && git push --tags",
    "version:minor": "npm version minor && git push --tags",
    "version:major": "npm version major && git push --tags"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "axios": "^1.6.2",
    "better-sqlite3": "^9.2.2",
    "date-fns": "^2.30.0",
    "electron-store": "^8.1.0",
    "googleapis": "^128.0.0",
    "node-cron": "^3.0.3",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^7.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@tanstack/react-query": "^5.12.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^7.0.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^5.0.1",
    "electron-notarize": "^1.2.2",
    "electron-rebuild": "^3.2.9",
    "electron-updater": "^6.1.7",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "googleapis-common": "^7.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.3",
    "vite": "^5.0.5"
  },
  "keywords": [
    "parkstay",
    "booking",
    "automation",
    "electron",
    "camping"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "build": {
    "extends": "./electron-builder.json"
  }
}
